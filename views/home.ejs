<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>OSIMS</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
    <link rel="stylesheet" href="/style1.css">
</head>
<body>
    <div class="container">
        <!-- left side start-->
         <aside>
            <!--start of top logo-->
            <div class="top">
                <div class="logo">
                    <img src="/images/company_logo-removebg-preview.png" alt="OMSIMS Logo" />
                </div>
                <div class="close">
                    <span class="material-symbols-outlined">close</span>
                </div>
            </div>
            <!--end of top logo-->

            <div class="sidebar">
                <a href="" class="active">
                    <span class="material-symbols-outlined">menu</span>
                    <h3>Dashboard</h3>
                </a>
                <a href=""> 
                    <span class="material-symbols-outlined">store</span>
                    <h3>Branches</h3>
                </a>
                <a href="">
                    <span class="material-symbols-outlined">person</span>
                    <h3>Users</h3>
                </a>
                <a href="">
                    <span class="material-symbols-outlined">history</span>
                    <h3>History</h3>
                </a>
                <a href="#" id="logoutLink">
                    <span class="material-symbols-outlined">logout</span>
                    <h3>Logout</h3>
                </a>
                
            </div>
         </aside>
        <!--left side end--> 

        <!--main section start-->
         <main>
            <h1>Dashboard</h1>

                <div class="date">
                    <input type="date">
                </div>

                <div class="insights">
                    <!-- Start selling -->
                        <div class="sales">
                            <span class="material-symbols-outlined">trending_up</span>
                                <div class="middle">
                                    <div class="left">
                                        <h3>Sales</h3>
                                        <h1>$25,000</h1>
                                    </div>
                                </div>
                                <div class="progress">
                                    <svg>
                                        <circle r="30" cy="40" cx="40"></circle>
                                    </svg>
                                    <div class="number">80%</div>
                                </div>
                        </div>
                        <small>Last 24 hours</small>
                    <!-- End selling -->
                    <!-- Start Expenses -->
                        <div class="expenses">
                            <span class="material-symbols-outlined">local_mall</span>
                                <div class="middle">
                                    <div class="left">
                                        <h3>Expenses</h3>
                                        <h1>$25,000</h1>
                                    </div>
                                </div>
                                <div class="progress">
                                    <svg>
                                        <circle r="30" cy="40" cx="40"></circle>
                                    </svg>
                                    <div class="number">80%</div>
                                </div>
                        </div>
                        <small>Last 24 hours</small>
                            <!-- End Expenses -->     
                                <!-- Start Income -->
                                <div class="income">
                                    <span class="material-symbols-outlined">stacked_line_chart</span>
                                        <div class="middle">
                                            <div class="left">
                                                <h3>Income</h3>
                                                <h1>$25,000</h1>
                                            </div>
                                        </div>
                                        <div class="progress">
                                            <svg>
                                                <circle r="30" cy="40" cx="40"></circle>
                                            </svg>
                                            <div class="number">80%</div>
                                        </div>
                                </div>
                                <small>Last 24 hours</small>
                            <!-- End Expenses -->                   

                </div>
         </main>
        <!--main start end-->

        <!--right side start-->
        <div class="right">
            <h1>right</h1>
         </div>
        <!--right side end-->
    </div>

    <script src="/script1.js"></script>
</body>
</html>
